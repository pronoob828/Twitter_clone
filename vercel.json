{
    "version":2,
    "builds":[
        {
            "src":"twitter_clone/wsgi.py",
            "use":"@vercel/python",
            "config":{"maxLambdaSize":"15mb","runtime":"python3.9"}
        }
    ],
    "routes":[
        {
            "src":"/(.*)",
            "dest":"twitter_clone/wsgi.py"
        }
    ]
}